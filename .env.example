APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_HOST_PORT=8000
DETAILED_LOGGING=false

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_HOST_PORT=3306
DB_DATABASE=dex
DB_USERNAME=dex
DB_PASSWORD=secret

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
ENABLE_QUEUE_SERVICE=0 # Set to 1 to enable the docker queue service
# QUEUE_CONNECTION=database # For normal use (remember to enable service for docker usage)
QUEUE_CONNECTION=sync # For debugging (stop queue service when debugging with sync)

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# DSS Data Exchange SOAP Configuration
DSS_WSDL_URL=https://dex.dss.gov.au/webservice?wsdl
DSS_USERNAME=your_username_here
DSS_PASSWORD=your_password_here
DSS_ORGANISATION_ID=your_org_id_here
DSS_LOGGING_ENABLED=true
DSS_LOG_CHANNEL=daily

# Debug Configuration (set to false in production)
DSS_DEBUG_WEB_DISPLAY=false
DSS_DEBUG_SHOW_REQUESTS=false
DSS_DEBUG_SHOW_RESPONSES=false
SHOW_DEBUG_INFORMATION=false
